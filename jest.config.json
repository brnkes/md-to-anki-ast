{
  "preset": "ts-jest/presets/default-esm",
  "moduleNameMapper": {
    "^(\\.{1,2}/.*)\\.js$": "$1"
  },
  "transform": {
    "^.+\\.[tj]sx?$": [
      "ts-jest",
      {
        "useESM": true
      },
      {
        "tsconfig": "tsconfig.test.json"
      }
    ]
  },
  "transformIgnorePatterns": ["<rootDir>/node_modules/"]
}